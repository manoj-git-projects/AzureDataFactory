{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-covid-reporting-azure"
		},
		"ls_blob_adlcovidreportingazure_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_blob_adlcovidreportingazure'"
		},
		"ls_blob_sacovidreportingazure_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blob_sacovidreportingazure'"
		},
		"ls_blob_adlcovidreportingazure_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlcovidreportingazure.dfs.core.windows.net"
		},
		"ls_http_opendata_europa_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().baseUrl}"
		},
		"tr_ingest_population_data_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/8027b605-cf3f-4128-810f-74cd5b6e22fd/resourceGroups/covid-reporting-rg/providers/Microsoft.Storage/storageAccounts/sacovidreportingazure"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-covid-reporting-azure_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_blob_adlcovidreportingazure_accountKey": {
						"value": "[parameters('ls_blob_adlcovidreportingazure_accountKey')]"
					},
					"ls_blob_sacovidreportingazure_connectionString": {
						"value": "[parameters('ls_blob_sacovidreportingazure_connectionString')]"
					},
					"ls_blob_adlcovidreportingazure_properties_typeProperties_url": {
						"value": "[parameters('ls_blob_adlcovidreportingazure_properties_typeProperties_url')]"
					},
					"ls_http_opendata_europa_eu_properties_typeProperties_url": {
						"value": "[parameters('ls_http_opendata_europa_eu_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-covid-reporting-azure_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"tr_ingest_population_data_properties_typeProperties_scope": {
						"value": "[parameters('tr_ingest_population_data_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-covid-reporting-azure_ArmTemplate_0')]"
			]
		}
	]
}